{
  "swagger" : "2.0",
  "info" : {
    "title" : "Store Catalog API",
    "description" : "An API for querying items catalog of Acme Inc.",
    "contact" : {
      "name" : "Laurent Broudoux",
      "url" : "http://github.com/lbroudoux",
      "email" : "laurent.broudoux@gmail.com"
    },
